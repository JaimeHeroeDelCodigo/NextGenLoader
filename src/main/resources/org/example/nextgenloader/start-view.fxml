<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>


<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.nextgenloader.HelloController">


  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
  <ImageView pickOnBounds="true" preserveRatio="false"
             fx:id="logoInitialScreen"
             layoutX="50"
             layoutY="50"
             fitWidth="200"
             fitHeight="150"
  >
    <Image url="@../../../logo.png" />
  </ImageView>

  <Label fx:id="welcomeText"/>
  <Button fx:id="openFileButton" text="Load new configuration" onAction="#openFileExplorerClick"/>
  <Button text="Continue existing loading" />
</VBox>
